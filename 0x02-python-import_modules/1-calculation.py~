#!/usr/bin/python3
from calculator_1 import add as a, sub as b, mul as c, div as d

if __name__ == "__main__":
    num1 = 10
    num2 = 5
    print("{:d} + {:d} = {:d}".format(num1, num2, a(num1, num2)))
    print("{:d} - {:d} = {:d}".format(num1, num2, b(num1, num2)))
    print("{:d} * {:d} = {:d}".format(num1, num2, c(num1, num2)))
    print("{:d} / {:d} = {:d}".format(num1, num2, d(num1, num2)))
